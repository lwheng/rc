set nocompatible
syntax on
set smartindent
set tabstop=2
set expandtab
set shiftwidth=2
set number
set list
set listchars=eol:~
set modeline
set ls=2
colorscheme delek
set statusline+=%F
set hlsearch
nmap 7 :vertical res-5<CR>
nmap 8 :vertical res+5<CR>
nmap 9 :res-5<CR>
nmap 0 :res+5<CR>
nmap <Tab> <C-w>w
